{
  "name": "rs-react-performance",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev:server": "ts-node --project tsconfig.node.json src/server/index.ts",
    "dev:vite": "vite",
    "dev:init": "npm run update-metrics && wait-on tcp:3001",
    "dev": "concurrently \"npm run dev:init\" \"npm run dev:server\" \"npm run dev:vite\"",
    "build": "tsc -b && vite build",
    "lint": "eslint . --config eslint.config.mjs",
    "preview": "vite preview",
    "format:fix": "prettier --write .",
    "prepare": "husky",
    "update-metrics": "ts-node --project tsnode.config.json src/utils/updateReadme.ts",
    "analyze-profile": "ts-node --project tsconfig.analyze.json src/utils/analyzeProfile.ts"
  },
  "dependencies": {
    "express": "^4.21.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-compiler": "^19.0.0-beta-3229e95-20250315",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.2",
    "wait-on": "^8.0.3"
  },
  "prettier": {
    "singleQuote": false,
    "endOfLine": "lf"
  }
}
